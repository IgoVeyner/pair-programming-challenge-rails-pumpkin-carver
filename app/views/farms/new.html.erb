<h1>List a New Farm</h1>

<%= form_with model: @farm do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit %>

<% end %>
